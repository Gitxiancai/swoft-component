#!/usr/bin/env php
<?php

// Disable run test application
define('RUN_TEST_APP', false);
define('WS_TEST_SERVER', true);

// Bootstrap
require dirname(__DIR__) . '/bootstrap.php';

// Run application
$app = new \SwoftTest\Component\Testing\TestApplication([
    'startConsole'        => true,
    'basePath'            => dirname(__DIR__),
    'beanFile'            => dirname(__DIR__) . '/testing/bean.php',
    'disabledAutoLoaders' => [
        \App\AutoLoader::class => 1,
    ],
]);

$app->run();
